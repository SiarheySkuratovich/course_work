<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="@string/pref_high_quality_key"
        android:title="@string/pref_high_quality_title"
        android:summary="@string/pref_high_quality_desc"/>
    <Preference
        android:key="@string/pref_about_key"
        android:title="@string/pref_about_title"
        android:summary="@string/pref_about_desc"/>
    <ListPreference
        android:defaultValue="3"
        android:entries="@array/audio_encoder_entries"
        android:entryValues="@array/audio_encoder_entry_values"
        android:key="@string/key_encoder"
        android:title="Set audio encoder"
        />
    <SeekBarPreference
        android:key="sampling"
        android:title="Set Audio Sampling Rate"
        android:summary="Summary."
        android:max="5"
        android:defaultValue="44100"/>
</PreferenceScreen>